# source
gpg --list-secret-keys --keyid-format=long
gpg --output ~/key.asc --export-secret-keys <id>
gpg --batch --export-secret-key <id> | ssh <ip_target_vm> gpg --batch --import

# target
gpg --version
apt install net-tools
apt install openssh-client
apt install openssh-server
ifconfig -a
export GPG_TTY=$(tty)
echo "test" | gpg --clearsign

# sign
gpgconf --launch gpg-agent

# backup
gpg --export-secret-keys --armor <id> > key.asc